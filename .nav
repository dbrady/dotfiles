# nav
_nav_to() {
  echo "Type nav to see all nav commands"
  echo -e "\033[36mcd \"$1\"\033[0m"
  cd "$1"
}

# alias nav="grep --color=always -P '(?<=^alias )[^=]+' ~/.nav"
alias nav='cat ~/.nav'

## Global Nav
alias cdb='_nav_to ~/bin'
alias cdd='_nav_to ~/devel/'
alias cdem='_nav_to ~/.emacs.d/personal/preload'
alias cds='_nav_to ~/devel/scrapbin'

## Dev/Tools Mine
if [[ -s ~/.nav.home ]]; then
  source ~/.nav.home;
fi


## Atlas
if [[ -s ~/.nav.work ]]; then
  source ~/.nav.work;
fi


echo ".nav loaded."
